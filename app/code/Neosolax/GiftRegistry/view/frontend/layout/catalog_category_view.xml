<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="https://use.fontawesome.com/releases/v5.7.1/css/all.css" src_type="url"/>
        <css src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"  src_type="url" rel="stylesheet" type="text/css"  />
<!--        <css src="css/bootstrap.css" rel="stylesheet" type="text/css"  />-->
    </head>
    <body>
        <referenceContainer name="content">
            <referenceBlock name="category.product.addto">
                <block class="Neosolax\GiftRegistry\Block\Giftregistry\Index"
                       name="category.product.addto.giftregistry" as="giftregistry" after="compare"
                       template="Neosolax_GiftRegistry::catalog/product/list/addto/giftregistry.phtml"
                       cacheable="false"
                />
            </referenceBlock>
        </referenceContainer>
    </body>
</page>
